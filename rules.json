[
    {
        "notation": "timestamp+bitcoin-transaction@metacode.biz",
        "verification": [
            "https://blockexplorer.com/api/tx/",
            ["load", "VALUE"],
            ["concat"],
            ["fetch", "json"],
            ["extract", "vout"],
            [
                "any", [
                    ["extract", ["scriptPubKey", "asm"]],
                    "OP_RETURN ",
                    ["load", "FINGERPRINT"],
                    ["concat"],
                    ["equals"]
                ]
            ]
        ]
    },
    {
        "notation": "proofs+github@metacode.biz",
        "verification": [
            "https://api.github.com/gists/",
            ["load", "VALUE"],
            ["concat"],
            ["fetch", "json"],
            ["duplicate"],
            ["extract", ["owner", "html_url"]],
            ["load", "USERID"],
            ["equals"],
            ["verify"],
            ["extract", "files"],
            ["any", [
                ["extract", "content"],
                "[Verifying my OpenPGP key: openpgp4fpr:",
                ["load", "FINGERPRINT"],
                ["concat"],
                "]",
                ["concat"],
                ["contains"]
            ]]
        ]
    },
    {
